{
  "version": 2,
  "buildCommand": "echo 'No build needed'",
  "rewrites": [
    {
      "source": "/api/contact",
      "destination": "/api/contact.js"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/website/assets/$1"
    },
    {
      "source": "/en/assets/(.*)",
      "destination": "/website-en/assets/$1"
    },
    {
      "source": "/fr/assets/(.*)",
      "destination": "/website-fr/assets/$1"
    },
    {
      "source": "/nl/assets/(.*)",
      "destination": "/website-nl/assets/$1"
    },
    {
      "source": "/pl/assets/(.*)",
      "destination": "/website-pl/assets/$1"
    },
    {
      "source": "/en/(.*)",
      "destination": "/website-en/$1"
    },
    {
      "source": "/fr/(.*)",
      "destination": "/website-fr/$1"
    },
    {
      "source": "/nl/(.*)",
      "destination": "/website-nl/$1"
    },
    {
      "source": "/pl/(.*)",
      "destination": "/website-pl/$1"
    },
    {
      "source": "/en",
      "destination": "/website-en/index.html"
    },
    {
      "source": "/fr",
      "destination": "/website-fr/index.html"
    },
    {
      "source": "/nl",
      "destination": "/website-nl/index.html"
    },
    {
      "source": "/pl",
      "destination": "/website-pl/index.html"
    },
    {
      "has": [
        {
          "type": "host",
          "value": "backup-generator.baltic-ihub.com"
        }
      ],
      "source": "/assets/(.*)",
      "destination": "/website-en/assets/$1"
    },
    {
      "has": [
        {
          "type": "host",
          "value": "backup-generator.baltic-ihub.com"
        }
      ],
      "source": "/(.*)",
      "destination": "/website-en/$1"
    },
    {
      "has": [
        {
          "type": "host",
          "value": "groupe-electrogene.baltic-ihub.com"
        }
      ],
      "source": "/assets/(.*)",
      "destination": "/website-fr/assets/$1"
    },
    {
      "has": [
        {
          "type": "host",
          "value": "groupe-electrogene.baltic-ihub.com"
        }
      ],
      "source": "/(.*)",
      "destination": "/website-fr/$1"
    },
    {
      "has": [
        {
          "type": "host",
          "value": "noodaggregaat.baltic-ihub.com"
        }
      ],
      "source": "/assets/(.*)",
      "destination": "/website-nl/assets/$1"
    },
    {
      "has": [
        {
          "type": "host",
          "value": "noodaggregaat.baltic-ihub.com"
        }
      ],
      "source": "/(.*)",
      "destination": "/website-nl/$1"
    },
    {
      "has": [
        {
          "type": "host",
          "value": "agregat-pradotworczy.baltic-ihub.com"
        }
      ],
      "source": "/assets/(.*)",
      "destination": "/website-pl/assets/$1"
    },
    {
      "has": [
        {
          "type": "host",
          "value": "agregat-pradotworczy.baltic-ihub.com"
        }
      ],
      "source": "/(.*)",
      "destination": "/website-pl/$1"
    },
    {
      "has": [
        {
          "type": "host",
          "value": "notstromaggregat.baltic-ihub.com"
        }
      ],
      "source": "/(.*)",
      "destination": "/website/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/website/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
